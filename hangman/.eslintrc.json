{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint-config-airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "singleQuote": true
  },
  "plugins": ["prettier", "import"]
}
